{
  "name": "http-aws-es",
  "version": "1.1.3",
  "description": "Use the elasticsearch-js client with Amazon ES",
  "repository": "https://github.com/TheDeveloper/http-aws-es",
  "bugs": "https://github.com/TheDeveloper/http-aws-es/issues",
  "keywords": [
    "elasticsearch",
    "elasticsearch-js",
    "aws-es",
    "aws",
    "amazon",
    "es",
    "http",
    "connector",
    "extension",
    "sign"
  ],
  "main": "index.js",
  "dependencies": {
    "aws-sdk": "^2.48.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build-node4; npm run build-node6; npm run build-legacy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-node4": "BABEL_ENV=node4 babel ./connector.js -o ./node4.js",
    "build-node6": "BABEL_ENV=node6 babel ./connector.js -o ./node6.js",
    "build-legacy": "BABEL_ENV=legacy babel ./connector.js -o ./index.js"
  },
  "author": "Geoff Wagstaff <geoff@gosquared.com>",
  "license": "MIT"
}
